{
  "name": "Oracle Error Passing",
  "version": "0.1",
  "description": "Used to get the data out of the error message docs",
  "background_page" : "background.html",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "content_scripts" : [
    {
        "matches" : ["http://*/*"],
        "js" : ["BlobBuilder.min.js", "FileSaver.min.js","jquery.min.js", "content.js"]
    }
  ]
}